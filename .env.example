# Copy this file to .env.local and fill in the values

# Admin credentials (run `npm run hash-password` to generate hash)
ADMIN_USERNAME=admin
# This is the bcrypt hash of your password. Default password is: admin123
# Run `node scripts/hash-password.js yourpassword` to generate a new hash
ADMIN_PASSWORD_HASH=$2b$12$LQv3c1yqBWVHxkd0LHAkCOYz6TgtFKK3NxMUFBo02YXdpqCx3KVXC

# Session secret - use a random 32+ character string
SESSION_SECRET=your-super-secret-session-key-change-this-in-production-32chars

# Data file path (relative to project root)
DATA_FILE_PATH=./data/songs.json
